{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "5000",
    "JWT_SECRET": "@jwt_secret",
    "DB_NAME": "mimuser",
    "DB_USER": "@db_user",
    "DB_PASS": "@db_pass",
    "DB_HOST": "@db_host",
    "FRONTEND_URL": "@frontend_url",
    "BACKEND_URL": "@backend_url"
  }
}